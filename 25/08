public class Clientes  {
    private int nroCuenta=0;
    private double saldo=0;
    public Clientes(int nroCuenta,double saldo){
        this.nroCuenta=nroCuenta;
        this.saldo=saldo;
    }
    public int getNroCuenta(){
        return nroCuenta;
    }
    public double getSaldo(){
        return saldo;
    }
    public void clientes (){
        if (nroCuenta==1){
            saldo =100000;
        }else if (nroCuenta==2){
            saldo =100000;
        } else if (nroCuenta==3){
            saldo =100000;
        }else if (nroCuenta==4){
            saldo =100000;
        }
    }

}
